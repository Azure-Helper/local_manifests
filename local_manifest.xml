<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!-- Remotes -->
	<remote name="azure"
          fetch="https://github.com/Azure-Helper" />
        
        <remote name="psos"
         fetch="https://github.com/PixysOS" />

        <remote name="psds"
         fetch="https://github.com/PixysOS-Devices" />

        <remote name="clang"
          fetch="https://gitlab.com/Panchajanya1999" />		  
  
    <!-- Device Tree -->
	<project name="device_xiaomi_raphael" path="device/xiaomi/raphael" remote="psds" revision="twelve" />
	
    <!-- Kernel Source -->
	<project name="kernel_xiaomi_raphael" path="kernel/xiaomi/raphael" remote="kernel" revision="staging" />

    <!-- Proprietary Blobs -->
	<project name="vendor_xiaomi_raphael" path="vendor/xiaomi/raphael" remote="psds" revision="twelve" />
	
    <!-- Toolchain -->
        <project name="azure-clang" path="prebuilts/clang/host/linux-x86/clang-azure" remote="clang" revision="main" />

    <!-- Device Settings -->
	<project name="packages_resources_devicesettings" path="packages/resources/devicesettings" remote="psos" revision="twelve" />

    <!-- Hardware Trees -->
        <project name="hardware_xiaomi" path="hardware/xiaomi" remote="psds" revision="twelve" />
	 
</manifest>
